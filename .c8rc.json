{

  "all": true,

  "include": [
    "src/**/*.js"
  ],

  "reporter": [
    "cobertura",
    "html",
    "text",
    "text-summary"
  ],

  "checkCoverage": true,

  "statements": 95,
  "branches": 85,
  "functions": 95,
  "lines": 95,

  "watermarks": {
    "statements": [75, 85],
    "branches": [75, 85],
    "functions": [75, 85],
    "lines": [75, 85]
  }
}